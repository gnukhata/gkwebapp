				  <td class="text-right">
				    {{ gkresult["invoicetotal"] }}
				  </td>
				</tr>
			      </tfoot>

			    </table>
			  </div>
			{% else %}
			  <div class="row">
			    <label for="tax applicable" name="taxflag"><b style="font-size:130%">{% trans %}Tax Applicable{% endtrans %} :</b></label>
			    <b style="font-size:130%">GST</b>
			  </div>
			  

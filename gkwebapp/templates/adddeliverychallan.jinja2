            <p>{% trans %}Are you sure?{% endtrans %}</p>
            <div id="invoice_number_input" hidden>
            	<form class="form-horizontal" >
            		<label>Enter the following details for invoice:</label>
            		<div class="form-group form-inline">
            			<label class="col-sm-4 fsize" for="invoice_challanno">{% trans %}Invoice No{% endtrans %} :<label  style="color:red; font-size:120%;">*</label></label>
            			<input class="form-control input-sm col-sm-8" size="15" id="invoice_number" maxlength="16" >
            		</div>
            		<div class="form-group form-inline">
            			<label for="invoice_date" class="col-sm-4 fsize">{% trans %}Invoice Date{% endtrans %} :<label style="color:red; font-size:120%;">*</label></label>
            			<input type="text" class="form-control input-sm invoicedate" size="2" maxlength="2" placeholder="DD" id="invoice_date" >
            			<input type="text" class="form-control input-sm invoicedate" size="2" maxlength="2" placeholder="MM" id="invoice_month" >
            			<input type="text" class="form-control input-sm invoicedate" size="4" maxlength="4" placeholder="YYYY" id="invoice_year">
            		</div>
            		<div class="form-group form-inline">
            			{% if gkstatus=="in" %}
            			<label for="invoicestate" class="col-sm-4 fsize">{% trans %}State of Destination{% endtrans %} :</label>
            			{% else %}
            			<label for="invoicestate" class="col-sm-4 fsize">{% trans %}State of Origin{% endtrans %} :</label>
            			{% endif %}
            			<select class="form-control input-sm col-sm-8" id="inv_invoicestate" aria-describedby="statecodeforinvoice">
            				{% for state in states %}
            				{% for key, value in state.items() %}
            				<option value="{{value}}" stateid="{{key}}">{{value}}</option>
            				{% endfor %}
            				{% endfor %}
            			</select>
            		</div>
					<div class="form-group form-inline">
		    <label class="col-sm-4 fsize" for="statecodeforinvoice">{% trans %}State Code{% endtrans %} :</label>
		    <label class="col-sm-8 nopadding fsize" id="inv_statecodeforinvoice"></label>
		  </div>
		  <div class="form-group form-inline invoice_issuer" >
		      <label class="col-sm-4 fsize gstinfield" for="orggstin">{% trans %}GSTIN{% endtrans %} :</label>
		      <label class="col-sm-8 nopadding fsize gstinfield" id="inv_orggstin"></label>
		    </div>
					<div class="form-group form-inline" >
						<label class="col-sm-4 fsize">{% trans %}Payment Details{% endtrans %} :</label>
		    <select class="form-control nopadding input-sm col-sm-8" id="chkpaymentmode" style="width:100px;">
		      <option value="3">
			{% trans %}Cash{% endtrans %}
		      </option>
		      <option value="2" >
			{% trans %}Bank{% endtrans %}
		      </option>
		      <option value="15" >
			{% trans %}On Credit{% endtrans %}
		      </option>
		    </select>
		  </div>
		  <div  style="display:none; margin-top:0.2cm;" id="bank">
		  <h5>Bank Details:</h5>
		    <div class="form-group form-inline">
		      <label for="accountno" class="col-sm-4">{% trans %}Account No.{% endtrans %} :</label>
		      <input type="text" id="accountno" class="lastfield form-control input-sm col-sm-8">
		  </div>
		    <div class="form-group form-inline">
		      <label for="bankname" class="col-sm-4">{% trans %}Bank Name{% endtrans %} :</label>
		      <input type="text" id="bankname" class="lastfield form-control input-sm col-sm-8" name="bankname" >
		    </div>
		  <br>
		    <div class="form-group form-inline">
		      <label for="branchname" class="col-sm-4">{% trans %}Branch Name{% endtrans %} :</label>
		      <input type="text" id="branchname" class="lastfield form-control input-sm col-sm-8" >
		    </div>
		    <div class="form-group form-inline">
		      <label for="ifsc" class="col-sm-4">{% trans %}IFSC code{% endtrans %} :</label>
		      <input type="text" id="ifsc" class="lastfield form-control input-sm col-sm-8" name="ifsc" >
		  </div>
		</div>
		<div class="form-group form-inline reversepurchase" id="reverse_charge">
		      <label for="inv" class="col-sm-4">{% trans %}Reverse Charge{% endtrans %}:</label>	
		      <div class="form-group col-sm-8 nopadding">
			<div class="radio">
        		  <label><input type="radio" class="iib lastfield form-control input-sm nopadding" name="iib" id="rev1radio" aria-label="">&nbsp {% trans %}Yes{% endtrans %}.</label>
        		</div>
			<div class="radio">
        		  <label><input type="radio" class="iib form-control input-sm nopadding" name="iib" id="rev2radio" aria-label="" checked>&nbsp {% trans %}No{% endtrans %}.</label>
        		</div>
		      </div>
			  </div>
            		<div id="delivery_out" hidden>
					<div class="form-group form-inline invoice_issuer">
            			<label for="originaddress" class="col-sm-4 fsize">{% trans %}Address :{% endtrans %}<label style="color:red; font-size:120%;">*</label></label>
            			<input type="text" id="originaddress" class="col-sm-8 form-control input-sm" name="originaddress" size="30"></input>
            		</div>
            			<div class="form-group form-inline invoice_issuer" >
            				<label class="col-sm-4 fsize" for="invoice_issuer_name">{% trans %}Issuer's Name{% endtrans %} :</label>
            				<input class="form-control input-sm col-sm-8" id="inv_issuer" placeholder="{% trans %}Issuer's name{% endtrans %}"  aria-label="{% trans %}Issuer's Name{% endtrans %}">
            			</div>
            			<div class="form-inline form-group invoice_issuer">
            				<label class="col-sm-4 fsize">{% trans %}Designation{% endtrans %} :</label>
            				<input class="form-control input-sm col-sm-8" id="inv_designation" aria-label="{% trans %}Designation{% endtrans %}" placeholder="{% trans %}Issuer's designation{% endtrans %}">
            			</div>
            		</div>
            	</form>
            </div>
			<label class="btn btn-primary btn-sm" for="inv_my-file-selector">
	  <input name="img" accesskey="h" class="input-sm" id="inv_my-file-selector" type="file" accept="image/*" multiple style="display:none;" onchange="$('#inv_upload-file-info').html(this.files.length+' selected');">
	  <i class="fa fa-paperclip" aria-hidden="true"></i>
	  {% trans %}Attac<u>h</u>{% endtrans %}
	</label>
	<span class='label label-info' id="inv_upload-file-info"></span>
	<small class="form-text text-danger text-muted">(JPG/JPEG images only.)</small>
        </div>
          <div class="modal-footer">
            <button type="button" id ="dc_save_no" class="btn btn-warning btn-sm" data-dismiss="modal">{% trans %}No{% endtrans %}</button>
            <button type="button" id="dc_save_yes" data-dismiss="modal" class="btn btn-success btn-sm">{% trans %}Yes{% endtrans %}</button>
          </div>
	</div>
      </div>
    </div>
  </div>
  <div id="confirm_yes_dc" class="modal" tabindex="-1" role="dialog">
    <div class="vertical-alignment-helper">
      <div class="modal-dialog modal-sm vertical-align-center">
        <div class="modal-content">
          <div class="modal-header">

            <h4 class="modal-title">{% trans %}Save?{% endtrans %}</h4>
          </div>
          <div class="modal-body">
            <p>{% trans %}Are you sure?{% endtrans %}</p>
          </div>
          <div class="modal-footer">
            <button type="button" id ="dc_save_no_print" class="btn btn-warning btn-sm" data-dismiss="modal">{% trans %}No{% endtrans %}</button>
            <button type="button" id="dc_save_yes_print" data-dismiss="modal" class="btn btn-success btn-sm">{% trans %}Yes{% endtrans %}</button>
          </div>
	</div>
      </div>
    </div>
  </div>

  <div id="viewcustsup"></div>
  <input type="hidden" id="selectedcustsup" value="">
  <script type="text/javascript" src="{{request.static_url('gkwebapp:static/js/numberToWords.js')}}"></script>
  <script type="text/javascript" src="{{request.static_url('gkwebapp:static/js/adddeliverychallan.js')}}"></script>

</body>
	</html>

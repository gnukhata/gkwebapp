    <style>
     .percentage:after {
	 content: "";
	 display: block;
	 background-color: #3d9970;
     }
     .percentage-20:after {
	 width: 20%;
     }
     .percentage-30:after {
	 width: 30%;
     }
     .bgcolor{
	 background-color:#1b2631;
     }
     #rss-feeddiv {
	 height: 60vh;
	 overflow-y: scroll;
     }
     .card-view .title {
	 font-weight: bold;
	 display: inline-block;
	 min-width: 30%;
	 text-align: left !important;
     }

     .dashtable tbody {
	 display:block;
	 height: 220px;
	 overflow-y:scroll;
     }
     .dashtable thead,.dashtable tbody tr {
	 display:table;
	 width:100%;
	 table-layout:fixed;
     }
     .dashtable thead {
	 width: calc( 97.6% );
     }
     .chartscroll{
	 overflow:auto;
	 height: 300px;
	 margin-bottom:0px;
	 padding-bottom: 0px;
     }
    </style>

    <style id="antiClickjack">body{display:none !important;}</style>

              {% trans %}Supply Type{% endtrans %}
            </th>
          </tr>
        </thead>
        <tbody>
          {% for invoice in cdnur %}
            <tr>
              <td class="calign">
                {{ invoice["ur_type"] }}
              </td>
              <td class="calign">
                {{ invoice["voucher_number"] }}
              </td>
              <td class="calign">
                {{ invoice["voucher_date"] }}
              </td>
              <td class="calign">
                {{ invoice["document_type"] }}
              </td>
              <td class="calign">
                {{ invoice["invoice_number"] }}
              </td>
              <td class="calign">
                {{ invoice["invoice_date"] }}
              </td>
              <td class="calign">
                {{ invoice["place_of_supply"] }}
              </td>
              <td class="ralign" >
                {{ invoice["refund_voucher_value"] }}
              </td>
              <td class="ralign">
                {{ invoice["applicable_tax"] }}
              </td>
              <td class="ralign">
                {{ invoice["rate"] }}
              </td>

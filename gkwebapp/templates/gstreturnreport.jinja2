              <td class="calign">
                {{ invoice["invoice_number"] }}
              </td>
              <td class="calign" >
                {{ invoice["invoice_date"] }}
              </td>
              <td class="ralign">
                {{ invoice["invoice_value"] }}
              </td>
              <td class="calign">
                {{ invoice["place_of_supply"] }}
              </td>
              <td class="ralign">
                {{ invoice["applicable_tax"] }}
              </td>
              <td class="ralign">
                {{ invoice["rate"] }}
              </td>
              <td class="ralign">
                {{ invoice["taxable_value"] }}
              </td>
              <td class="ralign">
                {{ invoice["cess"] }}
              </td>
              <td class="calign">
                {{ invoice["ecommerce_gstin"] }}
              </td>
              <td class="calign">
                {{ invoice["sale_from_bonded_wh"] }}
              </td>
            </tr>
          {% endfor %}
        </tbody>
      </table>
    </div>


    <div id="b2cs-container" hidden>
      <table class="table shadow table-bordered">
        <thead class="bg-primary">
          <tr>

<div class="col-sm-6">
  <div class="form-group " >
    <label >{% trans %}Transfer Note No:{% endtrans %} {{transfernote["transfernoteno"]}}</label>
  </div>
  <div class="form-group " >
    <label >{% trans %}Date:{% endtrans %} {{transfernote["transfernotedate"]}}</label>
  </div>
	      <div class="form-group">
		<label >{% trans %}Expected Date of Receipt :{% endtrans %} {% if transfernote["duedate"] %} {{transfernote["duedate"]}}{% else %} N/A{% endif %}</label>
              </div>
	      <div class ="form-group">
         	<label >{% trans %}Grace Period :{% endtrans %} {% if transfernote["grace"] %} {{transfernote["grace"]}}</label>
		  <small >   <b> days</b></small>{% else %} N/A</label>{% endif %}
	      </div>

	    <div class="form-group"  >
	      <label for="received_transfernote_date">{% trans %}Received Date:{% endtrans %} {% if transfernote["receiveddate"] %} {{transfernote["receiveddate"]}}{% else %} N/A{% endif %}</label>
            </div>

  <div class="form-group " >
    <div class="panel">
      <table id="transfernote_product_table" class="table table-condensed table-borderd fixed-table">
	<thead class="bg-primary">
          <tr>
            <th class="col-xs-9">{% trans %}Product{% endtrans %}</th>
            <th class="col-xs-3">{% trans %}Quantity{% endtrans %}</th>

          </tr>
	</thead>
	<tbody>

	</tbody>
      </table>
    </div>
  </div>
</div>
</div>



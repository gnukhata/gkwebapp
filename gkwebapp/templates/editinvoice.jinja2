		</tfoot>

	      </table>
	    </div>
	  </div>

	  <div class="col-md-12">
	    <div class="col-md-4">
	      <table class="table  table-condensed table-bordered table-striped">
		<tbody>
		  <h5>Bank Details:</h5>
		  <tr>
		    <td>  <label for="Account Number" class=" col-form-label col-form-label-sm" id="accountno">{% trans %}Account No. :{% endtrans %}</label></td>
		    <td colspan="2"><input type="text" id="accountno" class=" form-control input-sm" name="accountno" ></input></td>
		  </tr>
		  <tr>
		    <td>  <label for="Bank Name" class=" col-form-label col-form-label-sm" id="bankname">{% trans %}Bank Name :{% endtrans %}</label></td>
		    <td colspan="2"><input type="text" id="bankname" class=" form-control input-sm" name="bankname" ></input></td>
		  </tr>
		  <tr>

		    <td>  <label for="Bank Branch IFSC code" class=" col-form-label col-form-label-sm" id="ifsc">{% trans %}Bank Branch IFSC code :{% endtrans %}</label></td>
		    <td colspan="2"><input type="text" id="ifsc" class=" form-control input-sm" name="ifsc" ></input></td>

		  </tr>
		</tbody>
	      </table>

	    </div>
	    <div class="col-md-8" >
	      <h5>Total</h5>
	      <table class="table  table-condensed table-bordered table-striped">
		<tbody>

		  <tr>
		    <td>  <label style="font-size: 125%" for="total invoice value" class=" col-form-label col-form-label-sm" id="totalinvoicevalue">{% trans %}Total Invoice Value :{% endtrans %}</label></td>
		    <td colspan="2"></td>
		  </tr>
		</tbody>
	      </table>
	      <div class="row">
		<div class="col-md-6">
		  <table class="table  table-condensed table-bordered table-striped">
		    <tbody>
		      <tr>      <td>
  			<label for="transportationmode" id="transmode">{% trans %}Transportation Mode{% endtrans %} :</label>
		      </td>
		      <td>
  			<input type="text" id="transportationmode" class=" form-control input-sm" name="rvc" ></input>

		      </td>
		      </tr>
		      <tr>
			<td>

  			  <label for="inv" >{% trans %}Vehicle No.{% endtrans %} :</label>
			</td>
			<td>
  			  <input type="text" id="vehicleno" class="form-control input-sm" name="vehicleno"></input>

			</td>
		      </tr>
		    </tbody>
		  </table>
		</div>
		<div class="col-md-6">
		  <table class="table  table-condensed table-bordered table-striped">
		    <tbody>
		      <tr>
			<td>

  			  <label for="supply_date" >{% trans %}Date of Supply{% endtrans %} :</label>
			</td>
			<td>
			  <div class="form-group">
			    <input type="text" class="form-control input-sm supplydate" size="2" maxlength="2" placeholder="DD" id="supply_date" accesskey="v">

			    <input type="text" class="form-control input-sm supplydate" size="2" maxlength="2" placeholder="MM" id="supply_month" accesskey="v">

			    <input type="text" class="form-control input-sm supplydate" size="4" maxlength="4" placeholder="YYYY" id="supply_year" accesskey="v">
  			  </div>
			</td>
		      </tr>

		      <tr>

			<td>  <label for="inv" >{% trans %}Reverse Charge {% endtrans %} :</label></td>
			<td><input type="text" id="reversecharge" class="form-control input-sm" name="reversecharge"></input></td>

		      </tr>
		    </tbody>
		  </table>
		</div>
	      </div>
	    </div>
	  </div>
	</form>
      </div>
      </div>
    </div>






    
  </div>
  
  <div class="panel-footer">
    <div class="container-fluid btndisablediv" hidden>

      <button class="btn btn-success btn-sm" id="invoice_save" data-toggle="tooltip" data-placement="top" title="Press Insert anywhere on the current screen to save">{% trans %}Save{% endtrans %}</button>
<!--          <button id="invcl" class="btn btn-danger btn-sm" accesskey="d">{% trans %}Cancel{% endtrans %}</button>-->
          <button id="invoice_editprint" class="btn btn-primary btn-sm" >{% trans %}Print{% endtrans %}</button>
          <button class="btn btn-info btnfocus btn-sm" id="viewattach" accesskey="h">{% trans %}View Attac<u>h</u>ment{% endtrans %}</button>
        </div>
      </div>

      <div id="m_confirmdel" class="modal fade" tabindex="-1" role="dialog">
        <div class="vertical-alignment-helper">
          <div class="modal-dialog modal-sm vertical-align-center">
            <div class="modal-content">
              <div class="modal-header">

                <h4 class="modal-title">{% trans %}Cancel?{% endtrans %}</h4>
              </div>
              <div class="modal-body">
                <p>{% trans %}Are you sure?{% endtrans %}</p>
              </div>
              <div class="modal-footer">
                <button type="button" id ="m_cancel" class="btn btn-warning" data-dismiss="modal">{% trans %}No{% endtrans %}</button>
                <button type="button" id="invcancel" data-dismiss="modal" class="btn btn-danger">{% trans %}Yes{% endtrans %}</button>
              </div>
            </div><!-- /.modal-content -->
          </div><!-- /.modal-dialog -->
        </div>
      </div><!-- /.modal -->

  <script type="text/javascript" src="{{request.static_url('gkwebapp:static/js/editinvoice.js')}}"></script>

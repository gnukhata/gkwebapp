		    <td class="ratewrap"><input type="text" class="invoice_product_cgstrate form-control input-sm text-right numtype ratewrap" size="6" value="0.00" placeholder="0.00" disabled></td>
		    <td class="smwrap"><input type="text" class="invoice_product_cgstamount form-control input-sm text-right numtype smwrap" size="6" value="0.00" placeholder="0.00" disabled></td>

		    <td class="ratewrap"><input type="text" class="invoice_product_sgstrate  form-control input-sm text-right numtype ratewrap" size="6" value="0.00" placeholder="0.00" disabled></td>
		    <td class="smwrap"><input type="text" class="invoice_product_sgstamount  input-sm form-control text-right numtype smwrap" size="6" value="0.00" placeholder="0.00" disabled></td>


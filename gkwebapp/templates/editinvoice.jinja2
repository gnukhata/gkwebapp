	    <td>        <label for="invoice_supplieraddr">
              {% trans %}Supplier Address{% endtrans %} :
            </label>
            </td>
            <td colspan="2"><label id="invoice_supplieraddr" >{% trans %} {% endtrans %} </label></td>
          {% else %}

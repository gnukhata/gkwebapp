	    <label for="invoice_customer" id ="custsupname" ></label>
          </td>
	</tr>
	<tr>

	  <td>  <label for="gstin" class=" col-form-label col-form-label-sm" id="transmode">{% trans %}GSTIN / TIN:{% endtrans %}</label></td>

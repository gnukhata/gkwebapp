


<!DOCTYPE html>
<title>{% trans %}GNUKhata{% endtrans %} | {% trans %}INVOICE{% endtrans %}</title>
<style type="text/css">

 .color {
     background-color: 787878
 }
 #invoice_product_table_gst td {
     border-left: 1px solid #000;
     border-right: 1px solid #000;
 }

 #invoice_product_table_gst td:first-child {
     border-left: none;
 }

 #invoice_product_table_gst td:last-child {
     border-right: none;
 }
 #invoice_product_table_gst tbody {

     display:table;
     width: 89%
     height: calc(99vh - 22em);
     overflow-y:scroll;
 }
 #invoice_product_table_gst thead {
     display:table;
     width: 99%;
 }
 #invoice_product_table_gst tfoot {
     display:table;
     width: 99%;
 }
 #invoice_product_table_vat tfoot {
     display:table;
     width: 95%;
 }

 .nopadding{
     padidng:0 !important;
     padding:0 !important;
 }
 td {
     font-size: 65%;
     width: 10%;
     color: black;
 }

 th {
     font-size: 10%;
 }

 @page {
     size : 21cm 29.7cm;
     margin : 10mm 10mm 20mm 10mm
 }
 thead{
     display: table-header-group;
 }
 tfoot{
     display: table-footer-group;
 }
 .narwrap{
     width :10%;
 }
 .lngwrap{
     width:50%;
 }
 .mdwrap{
     width:25%;
 }
 .vlngwrap{
     width:80%;
 }
 .totalwrap {
     width:28%;
 }
 .rightJustified {
     text-align: right;
 }

</style>


<body>

  
  <div class="panel invoice_panel shadow">
    <div class="panel-body">
      <div class="container-fluid row">
        
        
	<!-- if taxflag==22(VAT) then only show this format of invoice -->
        <div class="form-group col-md-12">
          <label for="invoice_deliverynote">
            {% trans %}Invoice{% endtrans %} :
          </label>
          <select class="form-control input-sm" id="invoice_all_no" accesskey="">
            <option value="" disabled hidden selected>{% trans %}Select Invoice{% endtrans %}</option>
            {% for invoice in gkresult %}
              <option value="{{invoice['invid']}}">{{invoice["invoiceno"]}}, {{invoice["invoicedate"]}}, {{invoice["custname"]}}</option>
            {% endfor %}
          </select>
          <div class="alert alert-danger nopadding" id="cancelmsg"  aria-live="rude" role="alert" hidden>
            <strong id="alertstrong"></strong>
          </div>
        </div>

        <div class="col-md-3 invdetails" hidden>

          <div class="form-group">
            <label for="invoice_deliverynote">
              {% trans %}Delivery Note{% endtrans %} :
            </label>
            <input class="form-control input-sm" id="invoice_deliverynote">
          </div>
        </div>

        <div class="col-md-2 invdetails" hidden>

          <div class="form-group">
            <label for="invoice_challanno">{% trans %}Invoice No{% endtrans %} :</label>
            <input class="form-control input-sm" id="invoice_challanno" placeholder="{% trans %}Enter invoice no{% endtrans %}" accesskey="a">
          </div>
        </div>
        <div class="col-md-2 invdetails"  hidden>

          <div class="form-group">
            <label for="invoice_date">{% trans %}Date{% endtrans %} :</label>
            <div class="form-inline ">
              <div class="form-group">
                <input type="text" class="form-control input-sm invoicedate" size="2" maxlength="2" placeholder="DD" id="invoice_date" accesskey="v">
              </div>
              <div class="form-group">
                <input type="text" class="form-control input-sm invoicedate" size="2" maxlength="2" placeholder="MM" id="invoice_month" accesskey="v">
              </div>
              <div class="form-group">
                <input type="text" class="form-control input-sm invoicedate" size="4" maxlength="4" placeholder="YYYY" id="invoice_year" accesskey="v">
              </div>
            </div>
          </div>

        </div>

        <div class=" col-md-12 invdetails">
          <div class="form-group">
          </div>
        </div>

        <div class="col-md-3 invdetails"  hidden>

          <div class="form-group">
            <label for="invoice_customer" class="supp" hidden>
              {% trans %}Supplier{% endtrans %} :
            </label>
            <label for="invoice_customer" class="cust" hidden>
              {% trans %}Customer{% endtrans %} :

            </label>
            <select class="form-control input-sm" id="invoice_customer" accesskey="">

            </select>
          </div>
        </div>

        <div class="col-md-3 invdetails" hidden>
          <div class="form-group">
            <label for="invoice_customeraddr" class = "addr" hidden>
              {% trans %}Address{% endtrans %} :
            </label>
            <input class="form-control input-sm" id="invoice_customeraddr"  accesskey="" disabled>
          </div>
        </div>

        <div class="col-md-2 invdetails" hidden>
          <div class="form-group">
            <label for="invoice_customertin" class = "tin" hidden>
              {% trans %}TIN{% endtrans %} :
            </label>
            <input class="form-control input-sm" id="invoice_customertin" accesskey="" disabled>
          </div>
        </div>

        <div class="col-md-2 invstate" hidden>

          <div class="form-group">
            <label for="invoice_state">{% trans %}Vat for{% endtrans %} :</label>
            <select class="form-control input-sm tax_state product_new_state" id="invoice_state">
